<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.boes.testproject.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name="com.boes.testproject.app.MainActivity"
            android:label="@string/app_name" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name="com.boes.testproject.app.SecondActivity"
            android:label="@string/title_activity_second" >
        </activity>

        <service
            android:name=".NumberService"
            android:enabled="true"
            android:exported="false" >
        </service>

        <provider
            android:authorities="com.boes.testproject.app.provider"
            android:name="com.boes.testproject.app.DogContentProvider"
            android:enabled="true"
            android:exported="false">
        </provider>

    </application>

</manifest>
